<div class="md:flex md:items-center md:justify-between">
  <div class="flex-1 min-w-0">
    <h2 class="text-2xl font-bold leading-7 text-orange-900 sm:text-3xl sm:leading-9 sm:truncate">
      <%= @team.name %>
    </h2>
  </div>
  <div class="mt-4 flex md:mt-0 md:ml-4 items-center">
    <div class="mr-4">
      <span class="rounded-md shadow-sm">
        <%= link_to t('employeeRanking.backToDashboard'), admin_teams_url, class: "inline-flex items-center font-medium text-indigo-600 hover:text-indigo-500 transition ease-in-out duration-150" %>
      </span>
    </div>
  </div>
</div>
<div class="mt-6 bg-white px-4 py-5 border border-gray-200 rounded-md sm:px-6">
  <ul id="employees-list">
    <%= render @employees %>
    <%= paginate @employees %>
  </ul>
</div>
